<Window x:Class="WpfPractice.Views.HomeView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPractice.Views"
        mc:Ignorable="d"
        x:Name="HomeWindow"
        Title="HomeView" Height="450" Width="800">
    <Grid>
        <Button x:Name="ButtonRedirect" Content="Manage" Width="200" Height="100" Command="{Binding ButtonRedirect}" CommandParameter="{Binding ElementName=HomeWindow}"/>
    </Grid>
</Window>
